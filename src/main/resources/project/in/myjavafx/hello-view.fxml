<VBox prefHeight="713.0" prefWidth="862.0" spacing="20.0" style="-fx-background-color: 0,0,222; -fx-background-color: 100,200,299;" xmlns="http://javafx.com/javafx/22" xmlns:fx="http://javafx.com/fxml/1" fx:controller="project.in.myjavafx.HelloController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox minHeight="-Infinity" minWidth="-Infinity" prefHeight="75.0" prefWidth="3162.0">
      <children>
